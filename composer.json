{
    "name": "wpkin/flipbox",
    "description": " Flipbox - Awesomes Flip Boxes Image Overlay is the most easiest Flip builder Plugin. Create multiple Flip or  Flipboxes  with this.",
    "type": "wordpress-plugin",
    "license": "GPLv2",
    "authors": [
        {
            "name": "richardmallick",
            "email": "richardsetu@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "freemius/wordpress-sdk": "dev-master"
    },
	"require-dev": {
        "wp-coding-standards/wpcs": "^3.0",
		"dealerdirect/phpcodesniffer-composer-installer": "^1.0",
		"phpcompatibility/phpcompatibility-wp": "dev-master",
		"wp-phpunit/wp-phpunit": "^6.0",
		"yoast/phpunit-polyfills": "^1.0",
        "doctrine/instantiator": "^1.3.1",
		"phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "OXI_FLIP_BOX_PLUGINS\\" : ""
        }
    },
	"scripts": {
        "phpcs": [
            "phpcs -ps"
        ],
        "phpcs:fix": [
            "phpcbf -p"
        ],
        "test": [
            "phpunit"
        ],
        "test:all": [
            "phpcs -ps & phpunit"
        ]
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
